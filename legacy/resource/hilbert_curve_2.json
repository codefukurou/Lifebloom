{
  "name": "Hilbert Curve 2",
  "type": "cartesian-2d",
  "axiom": "X",
  "level": 4,
  "rules": {
      "X": "XFYFX+F+YFXFY-F-XFYFX",
      "Y": "YFXFY-F-XFYFX+F+YFXFY"
  },
  "functions": {
      "F": {
          "draw": true
      },
      "+": {
          "theta": 90.0
      },
      "-": {
          "theta": -90.0
      }
  }
}       