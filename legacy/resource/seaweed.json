{
  "name": "Ring",
  "type": "cartesian-2d",
  "axiom": "F",
  "level": 4,
  "rules": {
    "F": "FF-[-F+F+F]+[+F-F-F]"
  },
  "functions": {
    "F": {
      "draw": true
    },
    "+": {
      "theta": 22.0
    },
    "-": {
      "theta": -22.0
    },
    "[": {
      "store": true
    },
    "]": {
      "store": false
    }
  }
}