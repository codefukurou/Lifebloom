{
  "name": "Peano-Gosper Curve",
  "type": "cartesian-2d",
  "axiom": "XF",
  "level": 4,
  "rules": {
      "X": "X+YF++YF-FX--FXFX-YF+",
      "Y": "-FX+YFYF++YF+FX--FX-Y"

  },
  "functions": {
      "F": {
          "draw": true
      },
      "+": {
          "theta": 60.0
      },
      "-": {
          "theta": -60.0
      }
  }
}       